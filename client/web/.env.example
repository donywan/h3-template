# 开发环境配置
NODE_ENV=development

# API 服务器配置
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=10000

# 应用配置
VITE_APP_TITLE=H3 Template Web App
VITE_APP_DESCRIPTION=SolidStart frontend for H3 template powered by Bun

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEBUG=true

# 认证配置
VITE_JWT_STORAGE_KEY=h3_access_token
VITE_REFRESH_TOKEN_KEY=h3_refresh_token

# 微信小程序配置 (如果需要)
VITE_WECHAT_APP_ID=your_wechat_app_id
